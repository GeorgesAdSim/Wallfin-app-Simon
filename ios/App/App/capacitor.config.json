{
	"appId": "com.adsim.wallfin",
	"appName": "Wallfin",
	"webDir": "dist",
	"server": {
		"androidScheme": "https",
		"iosScheme": "https"
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 2000,
			"backgroundColor": "#f97316",
			"showSpinner": false,
			"androidSpinnerStyle": "small",
			"iosSpinnerStyle": "small"
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"LocalNotifications": {
			"smallIcon": "ic_stat_icon_config_sample",
			"iconColor": "#f97316"
		},
		"Keyboard": {
			"resize": "body",
			"style": "dark",
			"resizeOnFullScreen": true
		},
		"StatusBar": {
			"style": "light",
			"backgroundColor": "#f97316"
		}
	},
	"packageClassList": [
		"AppPlugin",
		"HapticsPlugin",
		"KeyboardPlugin",
		"LocalNotificationsPlugin",
		"PushNotificationsPlugin",
		"SplashScreenPlugin",
		"StatusBarPlugin"
	]
}
